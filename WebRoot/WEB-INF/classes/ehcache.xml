
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd">
	<diskStore path="java.io.tmpdir" />

	<cacheManagerEventListenerFactory class="" properties="" />
	
	<defaultCache maxElementsInMemory="20000" eternal="false"
		timeToIdleSeconds="7200" timeToLiveSeconds="7200" overflowToDisk="true"
		diskSpoolBufferSizeMB="50" maxElementsOnDisk="10000000"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="7200"
		memoryStoreEvictionPolicy="LRU" />
		
	<cache name="DEFAULT_CACHE" maxElementsInMemory="20000" eternal="false"
		timeToIdleSeconds="120" timeToLiveSeconds="120" overflowToDisk="true"
		diskSpoolBufferSizeMB="50" maxElementsOnDisk="10000000"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="7200"
		memoryStoreEvictionPolicy="LRU" />

</ehcache>

